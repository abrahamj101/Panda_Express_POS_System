{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["./src"], 
      "includePattern": ".js$",
      "exclude": ["node_modules", "docs"]
    },
    "plugins": [],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "encoding": "utf8",
      "destination": "./code-documentation", 
      "recurse": true,
      "verbose": true
    }
  }
  